{
  "name": "frontend",
  "version": "0.2.4",
  "private": true,
  "dependencies": {
    "abi-decoder": "^1.2.0",
    "antd": "3.10.9",
    "axios": "0.18.0",
    "bluebird": "3.5.3",
    "classnames": "2.2.6",
    "dayjs": "1.7.7",
    "detect-browser": "3.0.1",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "express-basic-auth": "1.1.6",
    "gridlex": "2.7.1",
    "history": "4.7.2",
    "include-media": "1.4.9",
    "lint": "1.1.2",
    "node-sass-chokidar": "1.3.4",
    "nodemon": "1.18.6",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-app-rewired": "1.6.2",
    "react-dom": "16.6.3",
    "react-facebook-pixel": "0.1.1",
    "react-ga": "2.5.5",
    "react-jazzicon": "0.1.3",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.5",
    "react-twitter-conversion-tracker": "1.0.0",
    "styled-components": "4.1.1",
    "web3": "1.0.0-beta.33",
    "websocket": "1.0.28",
    "@mybit/network.js": "file:../network.js"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "cross-env NODE_ENV=development npm run -p watch-css | npm run start-js",
    "build-js": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "build": "npm run build-css | npm run build-js",
    "run-test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore .",
    "now-start": "node server.js",
    "start-dev": "NODE_ENV=development nodemon --ignore ./src server.js",
    "lint:fix": "eslint --ignore-path .gitignore --fix ."
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-inline-react-svg": "1.0.1",
    "babel-runtime": "6.26.0",
    "compression-webpack-plugin": "2.0.0",
    "cross-env": "5.2.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "surge": "0.20.1",
    "ws": "6.1.2"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
