{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "carbon-components": "^8.18.5",
    "carbon-components-react": "^5.41.6",
    "carbon-icons": "^6.3.2",
    "classnames": "^2.2.5",
    "gridlex": "^2.7.1",
    "husky": "^0.14.3",
    "js-sha3": "^0.7.0",
    "lint-staged": "^7.0.0",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jazzicon": "0.0.1",
    "react-redux": "^5.0.7",
    "react-scripts": "^1.1.1",
    "redux-logger": "^3.0.6",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-react": "^0.78.3",
    "web3": "0.19.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "cross-env NODE_ENV=development npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "run-test": "react-scripts test --env=jsdom",
    "test": "npm-run-all build-css run-test",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --write src/*.js src/**/*.js",
    "eject": "react-scripts eject",
    "start-storybook": "cross-env start-storybook -p $PORT -c .storybook",
    "storybook": "cross-env PORT=8080 npm-run-all -p watch-css start-storybook",
    "run-storybook": "build-storybook -c .storybook -o .out",
    "build-storybook": "npm-run-all build-css run-storybook",
    "deploy-storybook:dev": "surge .out/ --domain storybook-mybit-dapp-ui-dev.surge.sh",
    "deploy-storybook:prod": "surge .out/ --domain storybook-mybit-dapp-ui.surge.sh",
    "deploy-storybook": "surge .out/",
    "deploy:dev": "surge build/ --domain mybit-dapp-ui-dev.surge.sh",
    "deploy:prod": "surge build/ --domain mybit-dapp-ui.surge.sh",
    "deploy": "surge build/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/addons": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "babel-core": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "surge": "^0.20.1",
    "ws": "3.3.2"
  }
}
